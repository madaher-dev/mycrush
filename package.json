    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "jssha": "^3.1.2",
    "mongoose": "^5.10.9",
